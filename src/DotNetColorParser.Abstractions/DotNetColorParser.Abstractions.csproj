<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Sebastian Felis</Authors>
    <Company>RoboLynx</Company>
    <Description>Abstractions foor .Net Standard library parsing color strings and converting them to .Net's Color object. 
Library support almost all color notations used in CSS3 and supported color notations are extendable with custom.</Description>
    <Copyright>Sebastian Felis Â© 2021</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/sebafelis/DotNetColorParser</PackageProjectUrl>
    <RepositoryUrl>https://github.com/sebafelis/DotNetColorParser</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>color parser css abstraction</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GitVersion.MsBuild" Version="5.6.6">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
